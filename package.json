{
  "name": "codigo-postal",
  "version": "3.0.0",
  "description": "Get postal code from Correos de Chile",
  "main": "src",
  "types": "src/types.d.ts",
  "scripts": {
    "lint": "eslint . --fix",
    "format": "prettier-standard '{src,test}/**/*.js'",
    "ts-compile-check": "tsc -p tsconfig.json --noEmit",
    "test": "nyc mocha test"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lgaticaq/codigo-postal.git"
  },
  "keywords": [
    "codigo postal",
    "postal code",
    "zip",
    "chile"
  ],
  "author": "Leonardo Gatica <lgatica@protonmail.com> (https://about.me/lgatica)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lgaticaq/codigo-postal/issues"
  },
  "homepage": "https://github.com/lgaticaq/codigo-postal#readme",
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@eclass/eslint-config": "1.3.1",
    "@eclass/semantic-release-npm-github-config": "3.0.1",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@types/chai": "4.2.11",
    "@types/mocha": "7.0.2",
    "@types/node": "12.12.49",
    "chai": "4.2.0",
    "eslint": "7.4.0",
    "eslint-plugin-array-func": "3.1.6",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsdoc": "26.0.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-sonarjs": "0.5.0",
    "eslint-plugin-standard": "4.0.1",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "mocha": "7.2.0",
    "nock": "12.0.3",
    "nyc": "15.1.0",
    "nyc-config-common": "1.0.1",
    "prettier-standard": "16.4.1",
    "semantic-release": "17.1.1",
    "typescript": "3.9.6"
  },
  "eslintConfig": {
    "extends": "@eclass"
  },
  "eslintIgnore": [
    "coverage",
    "example.js"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.js": [
      "eslint --fix",
      "prettier-standard",
      "git add"
    ]
  },
  "tonicExampleFilename": "example.js",
  "renovate": {
    "automerge": "minor",
    "extends": [
      "config:js-lib"
    ],
    "travis": {
      "enabled": true
    }
  },
  "release": {
    "extends": "@eclass/semantic-release-npm-github-config"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "nyc": {
    "extends": "nyc-config-common"
  }
}
