{
  "name": "codigo-postal",
  "version": "2.0.5",
  "description": "Get postal code from Correos de Chile",
  "main": "src",
  "scripts": {
    "lint": "eslint .",
    "format": "prettier-standard 'src/**/*.js' 'test/**/*.js'",
    "test": "nyc mocha test",
    "semantic-release": "semantic-release"
  },
  "engines": {
    "node": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lgaticaq/codigo-postal.git"
  },
  "keywords": [
    "codigo postal",
    "postal code",
    "zip",
    "chile"
  ],
  "author": "Leonardo Gatica <lgatica@protonmail.com> (https://about.me/lgatica)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lgaticaq/codigo-postal/issues"
  },
  "homepage": "https://github.com/lgaticaq/codigo-postal#readme",
  "dependencies": {
    "cheerio": "^1.0.0-rc.2"
  },
  "devDependencies": {
    "@commitlint/cli": "8.0.0",
    "@commitlint/config-conventional": "8.0.0",
    "@semantic-release/changelog": "3.0.4",
    "@semantic-release/commit-analyzer": "6.2.0",
    "@semantic-release/git": "7.0.13",
    "@semantic-release/github": "5.4.0",
    "@semantic-release/npm": "5.1.12",
    "@semantic-release/release-notes-generator": "7.2.0",
    "chai": "4.2.0",
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.0",
    "husky": "2.6.0",
    "lint-staged": "8.2.1",
    "mocha": "6.1.4",
    "nock": "10.0.6",
    "nyc": "14.1.1",
    "nyc-config-common": "1.0.1",
    "prettier-standard": "9.1.1",
    "semantic-release": "15.13.17"
  },
  "eslintConfig": {
    "extends": "standard",
    "env": {
      "mocha": true
    },
    "rules": {
      "no-console": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "coverage",
    "example.js"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "linters": {
      "src/**/*.js": [
        "eslint --fix",
        "prettier-standard",
        "git add"
      ],
      "test/**/*.js": [
        "eslint --fix",
        "prettier-standard",
        "git add"
      ]
    }
  },
  "tonicExampleFilename": "example.js",
  "renovate": {
    "automerge": "minor",
    "extends": [
      "config:base",
      ":pinOnlyDevDependencies"
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "nyc": {
    "extends": "nyc-config-common"
  }
}
